# .env.example
# API 설정
API_VERSION=v1
API_TITLE="VitalDB API"
API_PREFIX=/api/v1
DEBUG=True

# 데이터베이스 설정
POSTGRES_USER=your_user
POSTGRES_PASSWORD=your_password
POSTGRES_SERVER=localhost
POSTGRES_PORT=5432
POSTGRES_DB=vitaldb

# 외부 API 설정
VITALDB_API_URL=https://api.vitaldb.net
VITALDB_API_TIMEOUT=30

# CORS 설정
CORS_ORIGINS=["*"]
CORS_METHODS=["GET", "POST", "PUT", "DELETE", "OPTIONS"]
CORS_HEADERS=["*"]